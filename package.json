{
  "name": "docs-to-cli",
  "version": "0.1.0",
  "description": "Generate typed Bun clients from OpenAPI specs",
  "type": "module",
  "bin": {
    "docs-to-cli": "./index.ts"
  },
  "scripts": {
    "dev": "bun run index.ts",
    "test": "bun test",
    "build": "bun build --compile --target=bun-windows-x64 ./index.ts --outfile ./bin/dtoc.exe"
  },
  "keywords": ["openapi", "swagger", "bun", "cli", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
